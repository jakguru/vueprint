  "author": "Jak Guru <jak@jak.guru>",
  "license": "MIT",
  "scripts": {
    "watch": "nodemon",
    "build": "npx unbuild --sourcemap",
    "build:watch": "nodemon",
    "type-check": "npx tsc --noEmit",
    "lint": "eslint . --ext=.ts,.d.ts",
    "format": "eslint . --ext=.ts,.d.ts --fix",
    "docs:dev": "npx vitepress dev docs",
    "docs:build": "npx vitepress build docs",
    "docs:preview": "npx vitepress preview docs",
    "docs:api": "npx typedoc",
    "docs:api:watch": "npx typedoc --watch",
    "playground:vue:prepare": "cd playgrounds/vueprint-playground-vue && npm install",
    "playground:nuxt:prepare": "cd playgrounds/vueprint-playground-nuxt && npm install",
    "playground:vue": "npx nodemon --config nodemon-playground-vue.json",
    "playground:nuxt": "npx concurrently \"yarn build:watch\" \"npx nodemon --config nodemon-playground-nuxt.json\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "adonis-preset-ts": "^2.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.0",
    "less": "^4.2.0",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.0",
    "rollup": "^4.9.1",
    "rollup-plugin-styles": "^4.0.0",
    "stylus": "^0.63.0",
    "typedoc": "^0.25.11",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0",
    "vitepress": "^1.0.0-rc.45"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "warnOnUnsupportedTypeScriptVersion": false
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "dist",
    "node_modules",
    "*.tmp.*",
    "*.tmp"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "dependencies": {
    "@jakguru/milicron": "^1.0.0",
    "@mdi/font": "^7.4.47",
    "@nuxt/kit": "^3.10.3",
    "axios": "^1.6.7",
    "broadcast-channel": "^7.0.0",
    "defu": "^6.1.4",
    "dot-object": "^2.1.4",
    "firebase": "^10.8.1",
    "joi": "^17.12.2",
    "lodash.merge": "^4.6.2",
    "luxon": "^3.4.4",
    "notyf": "^3.10.0",
    "nuxt": "^3.10.3",
    "push.js": "^1.0.12",
    "secure-ls": "^1.2.6",
    "sweetalert2": "^11.10.6",
    "tiny-emitter": "^2.1.0",
    "tone": "^14.7.77",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-plugin-vuetify": "^2.0.2",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.1",
    "vuetify": "^3.5.7"
  }
}