      "types": "./dist/plugins/ui.d.ts"
    },
    "./plugins/vuetify": {
      "import": "./dist/plugins/vuetify.mjs",
      "require": "./dist/plugins/vuetify.cjs",
      "types": "./dist/plugins/vuetify.d.ts"
