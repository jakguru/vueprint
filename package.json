      "types": "./dist/plugins/api.d.ts"
    },
    "./plugins/bus": {
      "import": "./dist/plugins/bus.mjs",
      "require": "./dist/plugins/bus.cjs",
      "types": "./dist/plugins/bus.d.ts"
    },
    "./plugins/cron": {
      "import": "./dist/plugins/cron.mjs",
