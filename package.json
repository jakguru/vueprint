  "scripts": {
    "dev": "cd playground && npx vite",
    "watch": "nodemon",
    "build": "npx unbuild --sourcemap && npx jiti bin/updateDistributedPackageJson.ts",
    "build:watch": "nodemon",
    "type-check": "npx tsc --noEmit",
    "lint": "eslint . --ext=.ts,.d.ts",
    "format": "eslint . --ext=.ts,.d.ts --fix",
    "docs:dev": "npx vitepress dev docs",
    "docs:build": "npx vitepress build docs",
    "docs:preview": "npx vitepress preview docs",
    "docs:api": "npx typedoc",
    "docs:api:watch": "nodemon --config docs.api.nodemon.json"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "warnOnUnsupportedTypeScriptVersion": false
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "dist",
    "node_modules",
    "*.tmp.*",
    "*.tmp"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
